<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@color/bgForListQuran"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="in.tomtontech.markaz.Personal.CustomList_InstitutionDetailsIndividual">
    <TextView
        android:id="@+id/institutionDetails_individualName"
        style="@style/institutionDetails_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
         />

    <TextView
        android:id="@+id/institutionDetails_individualNo"
        style="@style/institutionDetails_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
